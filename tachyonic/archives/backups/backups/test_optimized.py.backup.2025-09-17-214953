#!/usr/bin/env python3
"""Test the optimized consciousness execution"""

from optimized_demo import OptimizedConsciousnessExecutor

def test_optimized_consciousness():
    print(" TESTING OPTIMIZED CONSCIOUSNESS EXECUTION")
    print("")
    
    executor = OptimizedConsciousnessExecutor()
    result = executor.simulate_consciousness_execution()
    
    print(f" Enhanced coherence: {result['enhanced_consciousness_coherence']:.6f}")
    print(f" Architectural bonus: {result['architectural_bonus']:.4f}")
    print(f" Method: {result['method']}")

if __name__ == "__main__":
    test_optimized_consciousness()
